*.tgz filter=lfs diff=lfs merge=lfs -text

# 3D Models
*.obj filter=lfs diff=lfs merge=lfs -text
# Large DLL files
*.dll filter=lfs diff=lfs merge=lfs -text
# PDF documentation
*.pdf filter=lfs diff=lfs merge=lfs -text
# Log files (if exist)
Assets/Logs/*.txt filter=lfs diff=lfs merge=lfs -text
Assets/StreamingAssets/logs/**/*.csv filter=lfs diff=lfs merge=lfs -text
